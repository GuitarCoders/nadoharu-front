{
  "name": "nadoharu-front",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn",
  "scripts": {
    "dev": "next dev",
    "dev:beta": "cross-env APP_ENV=beta next dev",
    "dev:prod": "cross-env APP_ENV=prod next dev",
    "build": "yarn codegen:dev && next build",
    "build:beta": "cross-env APP_ENV=beta yarn codegen:beta && next build",
    "build:prod": "cross-env APP_ENV=prod yarn codegen:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "cross-env APP_ENV=dev graphql-codegen --config codegen.ts",
    "codegen:beta": "cross-env APP_ENV=beta graphql-codegen --config codegen.ts",
    "codegen:prod": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.13.7",
    "@apollo/client-integration-nextjs": "^0.12.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-toast": "^1.2.13",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.12.1",
    "graphql": "^16.10.0",
    "iron-session": "^8.0.4",
    "jotai": "^2.12.3",
    "next": "15.3.0",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-virtuoso": "^4.13.0",
    "virtua": "^0.41.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/client-preset": "^4.8.1",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "tailwindcss": "^4",
    "typescript": "^5.8.3",
    "yarn": "^1.22.22"
  }
}
