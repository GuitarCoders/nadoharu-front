{
  "name": "nadoharu-front",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "next dev",
    "dev:beta": "cross-env APP_ENV=beta next dev",
    "dev:prod": "cross-env APP_ENV=prod next dev",
    "build": "yarn codegen && next build",
    "build:beta": "cross-env APP_ENV=beta yarn codegen:beta && next build",
    "build:prod": "cross-env APP_ENV=prod yarn codegen:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "cross-env APP_ENV=dev graphql-codegen --config codegen.ts",
    "codegen:beta": "cross-env APP_ENV=beta graphql-codegen --config codegen.ts",
    "codegen:prod": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e-ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/client-integration-nextjs": "^0.12.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-toast": "^1.2.13",
    "browser-image-compression": "^2.0.2",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.12.1",
    "graphql": "^16.10.0",
    "iron-session": "^8.0.4",
    "jotai": "^2.12.3",
    "next": "15.3.0",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-virtuoso": "^4.13.0",
    "sharp": "^0.34.3",
    "virtua": "^0.41.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/client-preset": "^4.8.1",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@playwright/test": "^1.54.2",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.4",
    "eslint-plugin-storybook": "^9.1.1",
    "jsdom": "^26.1.0",
    "storybook": "^9.1.1",
    "tailwindcss": "^4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "yarn": "^1.22.22",
    "@storybook/nextjs-vite": "^9.1.1",
    "@vitest/browser": "3.2.4",
    "playwright": "^1.54.2",
    "@vitest/coverage-v8": "3.2.4",
    "@storybook/test-runner": "^0.19.0"
    ,
    "vite": "^5.4.10"
  }
